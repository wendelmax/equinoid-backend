# Configurações do servidor
PORT=8080
GIN_MODE=debug
ENVIRONMENT=development

# Configurações do banco de dados PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USER=equinoid
DB_PASSWORD=equinoid123
DB_NAME=equinoid
DB_SSL_MODE=disable

# Configurações do Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Configurações JWT
JWT_SECRET=super-secret-jwt-key-change-in-production
JWT_EXPIRE_HOURS=24

# Configurações de segurança
BCRYPT_COST=12
RATE_LIMIT_PER_MINUTE=100

# Configurações de upload de arquivos
UPLOAD_MAX_SIZE=10485760
UPLOAD_PATH=./uploads

# Configurações AWS S3
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=equinoid-documents

# Configurações de logs
LOG_LEVEL=info
LOG_FILE=./logs/equinoid.log

# Configurações de certificados digitais
CA_CERT_PATH=./certs/ca-cert.pem
CA_KEY_PATH=./certs/ca-key.pem

# Configurações de monitoramento
METRICS_ENABLED=true
METRICS_PATH=/metrics

# Configurações de email
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
FROM_EMAIL=noreply@equinoid.org

# Configurações de integração externa
GEDAVE_API_URL=https://gedave.gov.br/api
GEDAVE_API_KEY=

# Configurações de blockchain
BLOCKCHAIN_ENABLED=false
ETHEREUM_RPC_URL=
ETHEREUM_PRIVATE_KEY=